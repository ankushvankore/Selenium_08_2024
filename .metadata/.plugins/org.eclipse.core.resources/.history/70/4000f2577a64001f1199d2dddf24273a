package com.FunctionDemos;

public class D06ReturningValueWithoutPassingArguments {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}
	
	public static void calSumOf1to10()
	{
		int i, sum = 0;
		
		for(i = 1; i <= 10; i++)
		{
			sum = sum + i;
		}
		
		return sum;
	}
}
